<% content_for :title do %>Licences - Performance Platform - GOV.UK<% end %>

<header>
  <p class="category-title">Licensing</p>
  <h1>Licences</h1>
</header>

<section id="licensing-overview">

  <ul id="licences">
    <% @licences.each do |licence| %>

      <li><%= link_to licence.name, licence_path(licence.slug) %></li>
    <% end %>
    
  </ul>
  
</section>
