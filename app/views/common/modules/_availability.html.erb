<% id ||= "availability" %>
<% title ||= "Service availability" %>
<section id="<%= id %>" class="graph double-decker-graph">
  <h1><%= title %></h1>
  <aside class='more-info'>
    <span class='more-info-link'>more info</span>
    <ul>
      <li>Data source: Pingdom</li>
      <li>Page load time measures the time taken for a sample page within the service to load.</li>
      <li>
        <a href="/service-manual/operations/uptime-and-availability.html" rel="external" target="_blank">Uptime</a>
        measures the percentage of time that the service was observed to be available.
      </li>
    </ul>
  </aside>
  <nav id="availability-nav"></nav>
  <h3>Page load time</h3>
  <p class="impact-number response-time"></p>
  <figure class="graph timeseries-graph response-time-graph"></figure>
  <h3>Uptime</h3>
  <p class="impact-number uptime"></p>
  <figure class="graph timeseries-graph uptime-graph"></figure>
</section>
