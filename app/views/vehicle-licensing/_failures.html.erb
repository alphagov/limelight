<% id ||= "failures" %>
<% title ||= "Recorded errors" %>
<section id="<%= id %>" class="failures">
  <h1><%= title %></h1>
  <aside class='more-info'>
    <span class='more-info-link'>more info</span>
    <ul>
      <li>Data source: <abbr title="Driver and Vehicle Licensing Agency">DVLA</abbr> database</li>
      <li>More than one error may be logged per user.</li>
    </ul>
  </aside>
  <h2>Errors generated during the online application process last week</h2>
  <article class="failures-table"></article>
</section>
