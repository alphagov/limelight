<% id ||= "volumetrics" %>
<% title ||= "Service Volumetrics" %>
<% description ||= "Service Volumetrics by month" %>
<% info_text ||= "The number of successful applications for a #{transaction_shortname}." %>
<section class="graph graph-with-labels vehicle-licensing-volumetrics">
  <h1><%= title %></h1>
  <aside class='more-info'>
    <span class='more-info-link'>more info</span>
    <ul>
      <li>Data source: <abbr title="Driver and Vehicle Licensing Agency">DVLA</abbr> database</li>
      <li><%=raw info_text %></li>
      <li>Digital: applications made online or using the automated telephone service</li>
      <li>Post Office: applications made at a Post Office branch</li>
      <li>DVLA centre: applications made at local DVLA centres</li>
    </ul>
  </aside>
  <h2 id="<%= id %>-headline"><%= description %></h2>
  <nav id="<%= id %>-nav"></nav>
  <figure class="graph timeseries-graph" id="<%= id %>"></figure>
</section>

