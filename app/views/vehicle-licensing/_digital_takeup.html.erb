<% id ||= "takeup" %>
<% title ||= "Digital take-up" %>
<% description ||= "Service Volumetrics by week" %>
<section class="graph single-decker-graph">
  <h1><%= title %></h1>
  <h2>Users completing the transaction online, including assisted digital users</h2>
  <ul class="module-actions">
    <li class="info">
      <div>
        <p>Data source: <abbr title="Driver and Vehicle Licensing Agency">DVLA</abbr> database</p>
        <p><a href="/service-manual/measurement/digital-takeup.html" rel="external" target="_blank">Digital take-up</a> measures the percentage of completed monthly applications that are made through a digital channel versus non-digital channels.</p>
      </div>
    </li>
  </ul>
  <p id="<%= id %>-number" class="impact-number"></p>
  <figure class="graph timeseries-graph" id="<%= id %>"></figure>
</section>